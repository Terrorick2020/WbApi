{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "./dist"
    },
    "include": [
      "src/**/*.ts",
      "knexfile.ts"
    ],
    "exclude": ["node_modules"]
}
