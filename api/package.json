{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/main.ts",
    "start": "ts-node src/server.ts",
    "knex:migrate:make": "npx knex --knexfile src/db/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "npx knex --knexfile src/db/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "npx knex --knexfile src/db/knexfile.ts migrate:rollback"
  },
