    "@types/node": "^22.13.1",
    "express": "^4.21.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.7",
    "express-async-handler": "^1.2.0",
    "helmet": "^8.0.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.2",
    "tsconfig-paths": "^4.2.0",
